extends layout
block variables
    include variables
block content
    br
    .container
        h1.mt-5 Удаление родителя
        br
        form(action='/delSchedule/'+val.idSubjTeacher, method='post')
            div(class="form-row")
                div(class="col-lg-4 col-md-4 col-sm-4 col-xs-6")
                    p() #{val.dayOfWeek} #{val.numPair} #{val.teacherName} #{val.subjectName} #{val.week} #{val.typeSubject}
                div(class="col-lg-12 col-md-12 col-sm-12 col-xs-6")
                    input(class="btn btn-danger" type='submit' value='Удалить')
                    font()=" "
                    a(class="btn btn-secondary" href='/listSchedule/'+val.groupId, role="button") Отмена
